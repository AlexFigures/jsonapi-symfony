{
  "name": "alexfigures/symfony-jsonapi-bundle",
  "description": "JSON:API 1.1 bundle for Symfony 7 with DX-first design",
  "type": "symfony-bundle",
  "license": "MIT",
  "homepage": "https://github.com/AlexFigures/jsonapi-symfony",
  "keywords": [
    "jsonapi",
    "symfony",
    "symfony-bundle",
    "api",
    "rest",
    "specification"
  ],
  "require": {
    "php": "^8.2",
    "symfony/dependency-injection": "^7.1",
    "symfony/config": "^7.1",
    "symfony/http-kernel": "^7.1",
    "symfony/event-dispatcher": "^7.1",
    "symfony/http-foundation": "^7.1",
    "symfony/routing": "^7.1",
    "symfony/property-access": "^7.1",
    "symfony/property-info": "^7.1",
    "symfony/serializer": "^7.1",
    "symfony/uid": "^7.1",
    "symfony/validator": "^7.1"
  },
  "require-dev": {
    "deptrac/deptrac": "^4.2",
    "doctrine/dbal": "^3.8",
    "doctrine/orm": "^3.0",
    "friendsofphp/php-cs-fixer": "^3.64",
    "infection/infection": "^0.31",
    "jbelien/phpstan-sarif-formatter": "^1.2",
    "phpstan/phpstan": "^1.11",
    "phpunit/phpunit": "^11.0",
    "rector/rector": "^1.0",
    "roave/backward-compatibility-check": "^8.14",
    "spatie/phpunit-snapshot-assertions": "^5.2",
    "symfony/cache": "^7.3",
    "symfony/phpunit-bridge": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "AlexFigures\\Symfony\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlexFigures\\Symfony\\Tests\\": "tests/",
      "AlexFigures\\Symfony\\StressApp\\": "scripts/stress/app/",
      "AlexFigures\\Symfony\\StressTest\\": "scripts/stress/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1-dev"
    }
  },
  "support": {
    "issues": "https://github.com/AlexFigures/jsonapi-symfony/issues",
    "discussions": "https://github.com/AlexFigures/jsonapi-symfony/discussions",
    "docs": "https://github.com/AlexFigures/jsonapi-symfony/tree/main/docs",
    "source": "https://github.com/AlexFigures/jsonapi-symfony"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/AlexFigures"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.4.12",
      "ext-sodium": "0"
    }
  }
}
